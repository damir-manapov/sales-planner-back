{
  "version": 2,
  "installCommand": "pnpm install",
  "buildCommand": "pnpm build",
  "builds": [
    {
      "src": "packages/api/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["packages/api/dist/**", "packages/shared/dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "packages/api/index.js"
    }
  ]
}
